var osname=Ti.Platform.osname;"android"===osname&&(Ti.Geolocation.Android.manualMode=!0,Titanium.Geolocation.accuracy=Titanium.Geolocation.ACCURACY_HIGH,gpsProvider=Ti.Geolocation.Android.createLocationProvider({name:Ti.Geolocation.PROVIDER_GPS,minUpdateTime:60,minUpdateDistance:100}),Ti.Geolocation.Android.addLocationProvider(gpsProvider));var runGeo=function(){1==Ti.Network.online?(Ti.Geolocation.purpose="Location is needed in order to get your weather.",Ti.Geolocation.getCurrentPosition(function(e){var o=e.coords.latitude,a=e.coords.longitude,t="http://api.wunderground.com/api/1f29607d2437a0f5/geolookup/conditions/forecast/q/"+o+","+a+".json",i=Ti.Network.createHTTPClient();i.onload=function(){var e=JSON.parse(this.responseText),o=e.forecast,a=(e.location,e.current_observation),t="Current City:",i="Current Time:",n="Current Weather:",r="Wind Direction:",d="Wind Speed MPH:",l="Current Temperature:",c="Feels Like:";loadInterface.buildHead(t),loadInterface.buildUI(a.display_location.city),loadInterface.buildHead(i),loadInterface.buildUI(o.txt_forecast.date),loadInterface.buildHead(n),loadInterface.buildUI(a.weather),loadInterface.buildHead(r),loadInterface.buildUI(a.wind_dir),loadInterface.buildHead(d),loadInterface.buildUI(a.wind_mph),loadInterface.buildHead(l),loadInterface.buildUI(a.temperature_string),loadInterface.buildHead(c),loadInterface.buildUI(a.feelslike_string)},i.open("GET",t),i.send()})):alert("Network currently unavailable.")};exports.runGeoCode=runGeo;