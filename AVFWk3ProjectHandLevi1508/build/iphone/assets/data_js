var read=function(){var e=[],a=Ti.Database.open("diablo3Db");a.execute("CREATE TABLE IF NOT EXISTS d3TBL (id INTEGER PRIMARY KEY, name TEXT, charId TEXT, level TEXT, charClass TEXT, seasonal TEXT)");for(var s=a.execute("SELECT * FROM d3TBL");s.isValidRow();)e.push({name:s.fieldByName("name"),charId:s.fieldByName("charId"),level:s.fieldByName("level"),charClass:s.fieldByName("charClass"),seasonal:s.fieldByName("seasonal")}),s.next();s.close(),a.close();var r=require("ui");r.savedChar(e)},save=function(e){var a=Ti.Database.open("diablo3Db");for(a.execute("CREATE TABLE IF NOT EXISTS d3TBL (id INTEGER PRIMARY KEY, name TEXT, charId TEXT, level TEXT, charClass TEXT, seasonal TEXT)"),i=0,j=e.length;j>i;i++)a.execute("INSERT INTO d3TBL (charClass, charId, level, name, seasonal) VALUES (?,?,?,?,?)",e[i].charClass,e[i].charId,e[i].level,e[i].name,e[i].seasonal);a.close()},del=function(){var e=Ti.Database.open("diablo3Db");e.execute("CREATE TABLE IF NOT EXISTS d3TBL (id INTEGER PRIMARY KEY, name TEXT, charId TEXT, level TEXT, charClass TEXT, seasonal TEXT)");for(var a=e.execute("SELECT * FROM d3TBL");a.isValidRow();)e.execute("DELETE FROM d3TBL WHERE id=?",a.fieldByName("id")),a.next();e.close()};exports.del=del,exports.save=save,exports.read=read;