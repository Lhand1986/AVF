var key="5nrdpc5px2auztjame7xu3adbsrz5u3g",getApi=function(e){var a="https://us.api.battle.net/d3/profile/"+e+"/?locale=en_US&apikey="+key;console.log(a);var r=Ti.Network.createHTTPClient({onload:function(){var e=JSON.parse(this.responseText),a=e.heroes,r=[];try{for(i=0,j=a.length;j>i;i++){var t={name:a[i].name,charId:a[i].id,level:a[i].level,charClass:a[i].class,seasonal:a[i].seasonal};r.push(t)}}catch(s){alert("Enter proper battletag format, please.")}var o=require("data");o.save(r)},onerror:function(){alert("There was an error processing your request. Please try again.")},timeout:5e3});r.open("GET",a),r.send()};exports.getApi=getApi;