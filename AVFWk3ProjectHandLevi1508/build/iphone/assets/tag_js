var battleTag,newChar=function(){var e=require("ui");if(console.log("UI New Char"),e.isAndroid){var a=Ti.UI.createTextField({hintText:"Enter Battle Tag in name-#### format (ie. Noob-1234)"}),r=Ti.UI.createAlertDialog({title:"Enter battle tag",androidView:a,buttonNames:["Okay"]});r.addEventListener("click",function(){console.log(a.value),battleTag=a.value;var e=require("api");e.getApi(battleTag)}),r.show()}else{var o=Ti.UI.createAlertDialog({title:"Enter Battle Tag as \nname-####",style:Ti.UI.iPhone.AlertDialogStyle.PLAIN_TEXT_INPUT});o.addEventListener("click",function(e){console.log("e.text "+e.text),battleTag=e.text;var a=require("api");a.getApi(battleTag)}),o.show()}};exports.newChar=newChar;