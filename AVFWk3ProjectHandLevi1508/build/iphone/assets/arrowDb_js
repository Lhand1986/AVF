var Cloud=require("ti.cloud");Cloud.debug=!0;var authenticate=function(){Cloud.Users.login({login:"admin",password:"1234"},function(e){console.log(e),e.success?console.log("success db"):alert("Error: "+e.message)})},dbFn=function(e){console.log("dbFn"),authenticate(),console.log(e)},save=function(e){for(console.log("db Save"),i=0,j=e.length;j>i;i++)Cloud.Objects.create({classname:"Diablo3Characters",fields:{name:e[i].name,charId:e[i].id,level:e[i].level,charClass:e[i].class,seasonal:e[i].seasonal}},function(e){e.success?console.log("Cloud saved!"):alert("Data not saved!")})};exports.auth=authenticate,exports.save=save,exports.dbFn=dbFn;