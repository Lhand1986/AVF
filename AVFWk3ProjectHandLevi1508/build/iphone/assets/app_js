var mainWin=Ti.UI.createWindow({backgroundColor:"#8C001A"});if(Ti.Network.online){var arrowDb=require("arrowDb");arrowDb.auth()}else Ti.Network.addEventListener("change",function(){if(Ti.Network.online){var e=require("arrowDb");e.auth()}});mainWin.addEventListener("open",function(){var e=Ti.Media.createSound({url:"identify.wav",volume:.1,looping:!1,allowBackground:!0});e.play()}),mainWin.open();var splash=require("splash");splash.splashFn();