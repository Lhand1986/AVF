var isAndroid="android"==Ti.Platform.osname?!0:!1,savedChar=function(e){var a=Ti.UI.createWindow({backgroundColor:"#8C001A"}),o=Ti.UI.createWindow({backgroundColor:"#8C001A"}),r=Ti.UI.createView({height:40,top:0,backgroundColor:"#626262"}),t=Ti.UI.createLabel({color:"#C6C6C6",text:"Diablo 3 Character Menu",font:{fontSize:25},left:20});"android"!=Ti.Platform.osname&&(r.top=20),r.add(t),o.add(r);var l=Ti.UI.createTableViewSection({});for(i=0,j=e.length;j>i;i++){var n=Ti.UI.createTableViewRow({font:{fontSize:30},borderColor:"#bdbdbd",borderWidth:1,color:"#C6C6C6",title:e[i].name,charId:e[i].charId,level:e[i].level,charClass:e[i].charClass,seasonal:e[i].seasonal});l.add(n)}var d=Ti.UI.createTableView({backgroundColor:"#8C001A",width:"90%",height:"80%",data:[l],top:50});"android"!=Ti.Platform.osname&&(d.top=70);var s=Ti.UI.createView({bottom:40,height:40,width:"100%",borderColor:"black",backgroundColor:"#ede1d1",id:"Close",zIndex:1}),c=Ti.UI.createLabel({text:"Close Window",color:"#000",backgroundColor:"#ede1d1",font:{fontSize:30,fontFamily:"helvetica",fontWeight:"bold"}});isAndroid===!1&&(s.add(c),o.add(s)),o.add(d),o.open(),o.addEventListener("click",function(e){if(console.log(e.source.title,e.source.charId,e.source.level,e.source.id),"Close"===e.source.id)o.close();else{var r=Ti.UI.createView({left:20,right:20,height:"40%",layout:"vertical"}),t=Ti.UI.createView({right:20,top:20,height:40,width:40,borderColor:"black"});if(e.source.seasonal===!0)var i="Yes";else var i="No";var l=Ti.UI.createLabel({color:"#C6C6C6",text:"Character Name: "+e.source.title}),n=Ti.UI.createLabel({color:"#C6C6C6",text:"Character Id: "+e.source.charId}),d=Ti.UI.createLabel({color:"#C6C6C6",text:"Level: "+e.source.level}),s=Ti.UI.createLabel({color:"#C6C6C6",text:"Class: "+e.source.charClass}),c=Ti.UI.createLabel({color:"#C6C6C6",text:"Seasonal: "+i}),T=Ti.UI.createLabel({textAlign:Ti.UI.TEXT_ALIGNMENT_CENTER,left:0,right:0,top:0,bottom:0,text:"X",backgroundColor:"Red",font:{fontSize:30,fontFamily:"helvetica",fontWeight:"bold"}});r.add(l),r.add(n),r.add(d),r.add(s),r.add(c),t.add(T),a.add(r),a.add(t),a.open(),t.addEventListener("click",function(){a.remove(r),a.remove(t),a.close()})}})};exports.isAndroid=isAndroid,exports.savedChar=savedChar;