var isAndroid="android"==Ti.Platform.osname?!0:!1,savedChar=function(e){var a=Ti.UI.createWindow({backgroundColor:"#F2F2F2"}),r=Ti.UI.createWindow({backgroundColor:"#F2F2F2"}),o=Ti.UI.createTableViewSection({headerTitle:"Characters"});for(i=0,j=e.length;j>i;i++){var t=Ti.UI.createTableViewRow({borderColor:"#bdbdbd",borderWidth:1,color:"#000",title:e[i].name,charId:e[i].charId,level:e[i].level,charClass:e[i].charClass,seasonal:e[i].seasonal});o.add(t)}var l=Ti.UI.createTableView({backgroundColor:"#F2F2F2",width:"90%",height:"95%",data:[o]}),n=Ti.UI.createView({bottom:40,height:40,width:"100%",borderColor:"black",backgroundColor:"#ede1d1",id:"Close",zIndex:1}),d=Ti.UI.createLabel({text:"Close Window",color:"#000",backgroundColor:"#ede1d1",font:{fontSize:30,fontFamily:"helvetica",fontWeight:"bold"}});isAndroid===!1&&(n.add(d),r.add(n)),r.add(l),r.open(),r.addEventListener("click",function(e){if(console.log(e.source.title,e.source.charId,e.source.level,e.source.id),"Close"===e.source.id)r.close();else{var o=Ti.UI.createView({left:20,right:20,height:"40%",layout:"vertical"}),t=Ti.UI.createView({right:20,top:20,height:30,width:30,borderColor:"black"});if(e.source.seasonal===!0)var i="Yes";else var i="No";var l=Ti.UI.createLabel({color:"#000",text:"Character Name: "+e.source.title}),n=Ti.UI.createLabel({color:"#000",text:"Character Id: "+e.source.charId}),d=Ti.UI.createLabel({color:"#000",text:"Level: "+e.source.level}),s=Ti.UI.createLabel({color:"#000",text:"Class: "+e.source.charClass}),c=Ti.UI.createLabel({color:"#000",text:"Seasonal: "+i}),T=Ti.UI.createLabel({text:"X",backgroundColor:"Red",font:{fontSize:30,fontFamily:"helvetica",fontWeight:"bold"}});o.add(l),o.add(n),o.add(d),o.add(s),o.add(c),t.add(T),a.add(o),a.add(t),a.open(),t.addEventListener("click",function(){a.remove(o),a.remove(t),a.close()})}})};exports.isAndroid=isAndroid,exports.savedChar=savedChar;