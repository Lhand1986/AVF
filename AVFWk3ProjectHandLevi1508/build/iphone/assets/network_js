var networkFn=function(e){if(0===e)if(Ti.Network.online){var a=require("tag");a.newChar()}else alert("Error, need active network connection to pull new data");if(2===e){var o=require("data");o.read()}if(4===e){var r=Ti.UI.createAlertDialog({title:"This will delete all saved data\n Are you sure?",buttonNames:["Confirm","Cancel"]});r.show(),r.addEventListener("click",function(e){if(0===e.index){var a=require("data");a.del()}})}};exports.networkFn=networkFn;