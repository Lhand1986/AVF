var splashFn=function(){var e=Ti.UI.createWindow({backgroundColor:"grey"}),a=Ti.UI.createLabel({textAlign:Ti.UI.TEXT_ALIGNMENT_CENTER,verticalAlign:Ti.UI.TEXT_VERTICAL_ALIGNMENT_CENTER,top:0,bottom:0,left:0,right:0,text:"Diablo 3 Character Data"});"android"!=Ti.Platform.osname&&(a.color="#C6C6C6"),e.add(a),e.open(),e.addEventListener("close",function(){var e=require("menu");e.menuFn()}),setTimeout(function(){e.close()},2e3)};exports.splashFn=splashFn;