var osname=Ti.Platform.osname,online=Ti.Network.online,location=Ti.Geolocation.locationServicesEnabled;"android"===osname&&(Titanium.Geolocation.accuracy=Titanium.Geolocation.ACCURACY_HIGH),console.log(osname,online,location);var getPosition=function(){online===!0&&location===!0?("android"===osname&&(Ti.Geolocation.purpose="Location is needed in order to get your weather."),Ti.Geolocation.getCurrentPosition(function(t){var e=t.coords.latitude,i=t.coords.longitude,a="http://api.wunderground.com/api/1f29607d2437a0f5/geolookup/conditions/forecast/q/"+e+","+i+".json",o=Ti.Network.createHTTPClient();o.onload=function(){Ti.App.Properties.setString("weatherJSON",this.responseText)},o.open("GET",a),o.send(),loadInterface.rnWthr()})):(Ti.API.info("offline"),loadInterface.rnWthr())};mainWin.addEventListener("swipe",function(t){"down"==t.direction&&getPosition()});